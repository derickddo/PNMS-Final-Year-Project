{
    "builds": [{
        "src": "core/wsgi.py",
        "use": "@vercel/python",
        "config": { "maxLambdaSize": "15mb", "runtime": "python3.11" },
        "commands": [
            "apt-get update && apt-get install -y libpq-dev gcc",
            "pip install --target . -r requirements.txt"
          ]
    }],
    "routes": [
        
        {
            "src": "/(.*)",
            "dest": "core/wsgi.py"
        }
    ]
    
}